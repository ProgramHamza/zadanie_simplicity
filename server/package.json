{
  "name": "server",
  "version": "1.0.0",
  "description": "Announcements backend API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "devStart": "nodemon dist/server.js",
    "prestart": "npm run prisma:push && npm run build",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "seed:mock": "tsx prisma/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "websocket"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.3.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.6",
    "@types/ws": "^8.5.13",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
